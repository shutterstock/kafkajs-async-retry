{
  "compilerOptions": {
    "incremental": true,
    "target": "es2022",
    "module": "node16",
    "rootDir": "./src",
    "lib": ["ES2022"],
    "outDir": "./dist",
    "declaration": false,
    "inlineSourceMap": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "exclude": ["./dist"]
}
